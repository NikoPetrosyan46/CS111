//Ask the user for two integers (up to 100 digits), compute, and print their sum.

#include <iostream>
using namespace std;

int main()
{

	int num0, num1, sum;
		cout << "Enter the first number: ";
    cin >> num0;
    cout << "Enter the second number: ";
    cin >> num1;
    
	sum = num0 + num1;
		cout << "The sum is: " << sum << endl;
    
	return 0;
}
